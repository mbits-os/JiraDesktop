msgctxt "LNG_APP_MENUITEM_NEWCONN"
msgid "New &Connection..."
msgstr "New &Connection..."

#. Tooltip for LNG_APP_MENUITEM_NEWCONN
msgctxt "LNG_APP_MENUITEM_NEWCONN_TIP"
msgid "New Connection"
msgstr "New Connection"

