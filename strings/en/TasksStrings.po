msgid "Tasks and issues"
msgstr "Tasks and issues"

#. Infotip text for program links.  %1 is LNG_APP_NAME, %2 is version with stability (-beta, -rc1, etc) and %3 is the build number
msgctxt "LNG_APP_INFOTIP"
msgid "%1 v%2, Build %3"
msgstr "%1 v%2, Build %3"

#. Negative answer from the dialog
msgctxt "LNG_APP_LOGIN_CANCEL"
msgid "Cancel"
msgstr "Cancel"

#. The message for the user. %1 is the server address, %2 is 'authentication realm' as sent in by server.
msgctxt "LNG_APP_LOGIN_MESSAGE"
msgid "Server %1 requests user name and password. The server says: %2."
msgstr "Server %1 requests user name and password. The server says: %2."

