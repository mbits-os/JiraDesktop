msgctxt "LNG_APP_MENUITEM_REFRESH"
msgid "&Refresh"
msgstr "&Refresh"

#. Tasks command. Begins process of refreshing all tables
msgctxt "LNG_APP_MENUITEM_REFRESHALL"
msgid "&Refresh All"
msgstr "&Refresh All"

#. Tooltip for LNG_APP_MENUITEM_REFRESHALL
msgctxt "LNG_APP_MENUITEM_REFRESHALL_TIP"
msgid "Refresh All"
msgstr "Refresh All"

