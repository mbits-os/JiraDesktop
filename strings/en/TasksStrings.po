msgid "Settings"
msgstr "Settings"

#. Tray command. Allows to switch the application between visible/invisible states
msgctxt "LNG_APP_MENUITEM_SHOWHIDE"
msgid "Show/hide Tasks"
msgstr "Show/hide Tasks"

#. Languages submenu. Used for action representing the default user language
msgctxt "LNG_APP_MENUITEM_SYSLANG"
msgid "&System language (%1)"
msgstr "&System language (%1)"

#. Top level menu name for connection commands
msgctxt "LNG_APP_MENU_CONNNECTIONS"
msgid "&Connections"
msgstr "&Connections"

#. Top level menu name for help commands
msgctxt "LNG_APP_MENU_HELP"
msgid "&Help"
msgstr "&Help"

#. Top level menu name for application commands
msgctxt "LNG_APP_MENU_TASKS"
msgid "&Tasks"
msgstr "&Tasks"

#. Application name for window title
msgctxt "LNG_APP_NAME"
msgid "Tasks"
msgstr "Tasks"

#. Full window title. %1 is LNG_APP_NAME, %2 is version and %3 is the build number
msgctxt "LNG_APP_TITLE"
msgid "%1 (%2, build %3)"
msgstr "%1 (%2, build %3)"

#. Window title for elevated runs. %1 is LNG_APP_NAME or LNG_APP_TITLE
msgctxt "LNG_APP_TITLE_ELEVATED"
msgid "Administrator: %1"
msgstr "Administrator: %1"

#. Tooltip title for attention icon
msgctxt "LNG_ATTENTION_TOOLTIP"
msgid "Report(s) changed"
msgstr "Report(s) changed"

#. Tooltip baloon notification name for added issues
msgctxt "LNG_JIRA_ADDED"
msgid "Added"
msgstr "Added"

#. Tooltip baloon notification name for changed issues
msgctxt "LNG_JIRA_CHANGED"
msgid "Changed"
msgstr "Changed"

#. Table footer presenting the data slice; as if '(Issues 1-25 of 30)'
msgctxt "LNG_JIRA_ISSUES"
msgid "(Issues %1-%2 of %3)"
msgstr "(Issues %1-%2 of %3)"

#. Column title for issue key - in case of field definition error; most of the time this string will come from JIRA server
msgctxt "LNG_JIRA_KEY"
msgid "Key"
msgstr "Key"

#. Tooltip baloon title for attention icon notification
msgctxt "LNG_JIRA_NOTIFY"
msgid "Changes in %1@%2"
msgstr "Changes in %1@%2"

#. Notification text for issue group. As if 'Added 4 issues'
msgctxt "LNG_JIRA_NOTIFY_GROUP"
msgid "%1 %2 issue"
msgid_plural "%1 %2 issues"
msgstr[0] "%1 one issue"
msgstr[1] "%1 %2 issues"

#. Notification text for issue group contents, used when group consists of more than two issues. As if 'KEY-1, KEY-2 and 5 others'
msgctxt "LNG_JIRA_NOTIFY_MORE"
msgid "%1, %2 and %3 other"
msgid_plural "%1, %2 and %3 others"
msgstr[0] "%1, %2 and one other"
msgstr[1] "%1, %2 and %3 others"

#. Notification text for issue group contents, used when group consists of two issues only. As if 'KEY-1 and KEY-2'
msgctxt "LNG_JIRA_NOTIFY_TWO"
msgid "%1 and %2"
msgstr "%1 and %2"

#. Tooltip baloon notification name for removed issues
msgctxt "LNG_JIRA_REMOVED"
msgid "Removed"
msgstr "Removed"

#. Replacement text for empty report
msgctxt "LNG_JIRA_REPORT_EMPTY"
msgid "Empty"
msgstr "Empty"

