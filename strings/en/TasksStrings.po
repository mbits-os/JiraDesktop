#. Main text for About dialog.  %1 is LNG_APP_NAME, %2 is version with fancy stability (-β, -rc1, etc), %3 is the build number and %4 is the copyright holder
msgctxt "LNG_APP_ABOUT_MESSAGE"
msgid ""
"%1 v%2, Build %3\n"
"\n"
"© 2015 %4"
msgstr ""
"%1 v%2, Build %3\n"
"\n"
"© 2015 %4"

#. Title for About dialog
msgctxt "LNG_APP_ABOUT_TITLE"
msgid "About Tasks"
msgstr "About Tasks"

#. Negative answer from the dialog
msgctxt "LNG_APP_CONNECTION_CANCEL"
msgid "Cancel"
msgstr "Cancel"

#. Positive answer from the dialog
msgctxt "LNG_APP_CONNECTION_OK"
msgid "Save"
msgstr "Save"

#. Label for server name edit box
msgctxt "LNG_APP_CONNECTION_SERVER"
msgid "&Name"
msgstr "&Name"

#. Title for Connection dialog
msgctxt "LNG_APP_CONNECTION_TITLE"
msgid "Connection"
msgstr "Connection"

#. Label for server url edit box
msgctxt "LNG_APP_CONNECTION_URL"
msgid "&URL"
msgstr "&URL"

#. Application description for VERSIONINFO
