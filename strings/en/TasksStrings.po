msgctxt "LNG_APP_LOGIN_OK"
msgid "Log in"
msgstr "Log in"

#. Label for password edit box
msgctxt "LNG_APP_LOGIN_PASSPHRASE"
msgid "&Pass phrase"
msgstr "&Pass phrase"

#. Title for Login dialog
msgctxt "LNG_APP_LOGIN_TITLE"
msgid "Password needed"
msgstr "Password needed"

#. Label for user name edit box
msgctxt "LNG_APP_LOGIN_USERNAME"
msgid "&Login"
msgstr "&Login"

#. Help command. Shows dialog with name, version and copyright info.
msgctxt "LNG_APP_MENUITEM_ABOUTTASKS"
msgid "&About Tasks..."
msgstr "&About Tasks..."

#. Tooltip for LNG_APP_MENUITEM_ABOUTTASKS
msgctxt "LNG_APP_MENUITEM_ABOUTTASKS_TIP"
msgid "About Tasks"
msgstr "About Tasks"

#. Connection command. Edits details of an exisiting connection
msgctxt "LNG_APP_MENUITEM_EDIT"
msgid "&Edit..."
msgstr "&Edit..."

#. Tasks command. Closes the application
msgctxt "LNG_APP_MENUITEM_EXIT"
msgid "E&xit"
msgstr "E&xit"

#. Tooltip for LNG_APP_MENUITEM_EXIT
msgctxt "LNG_APP_MENUITEM_EXIT_TIP"
msgid "Exit"
msgstr "Exit"

#. Connection command. Opens current issue in web browser
msgctxt "LNG_APP_MENUITEM_GOTO"
msgid "&Go To Issue"
msgstr "&Go To Issue"

#. Tasks submenu. Shows a list of available translations
msgctxt "LNG_APP_MENUITEM_LANGUAGES"
msgid "&Languages"
msgstr "&Languages"

#. Help command. Presents licences of all third party components
msgctxt "LNG_APP_MENUITEM_LICENSES"
msgid "Show &Licences"
msgstr "Show &Licences"

#. Tooltip for LNG_APP_MENUITEM_LICENSES
msgctxt "LNG_APP_MENUITEM_LICENSES_TIP"
msgid "Licences"
msgstr "Licences"

#. Connection command. Logs time spent with current issue
msgctxt "LNG_APP_MENUITEM_LOGWORK"
msgid "Log &Work..."
msgstr "Log &Work..."

#. Languages submenu. Used for disabled action representing no languages at all
msgctxt "LNG_APP_MENUITEM_MISSING"
msgid "(Empty)"
msgstr "(Empty)"

