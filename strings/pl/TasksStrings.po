#. Main text for About dialog.  %1 is LNG_APP_NAME, %2 is version with fancy stability (-β, -rc1, etc), %3 is the build number and %4 is the copyright holder
msgctxt "LNG_APP_ABOUT_MESSAGE"
msgid ""
"%1 v%2, Build %3\n"
"\n"
"© 2015 %4"
msgstr ""
"%1 v%2, Kompilacja %3\n"
"\n"
"© 2015 %4"

#. Title for About dialog
msgctxt "LNG_APP_ABOUT_TITLE"
msgid "About Tasks"
msgstr "Zadania - informacje"

#. Application description for VERSIONINFO
msgctxt "LNG_APP_DESCRIPTION"
msgid "Tasks and issues"
msgstr "Zadania i problemy"

#. Infotip text for program links.  %1 is LNG_APP_NAME, %2 is version with stability (-beta, -rc1, etc) and %3 is the build number
msgctxt "LNG_APP_INFOTIP"
msgid "%1 v%2, Build %3"
msgstr "%1 v%2, Kompilacja %3"

#. Negative answer from the dialog
msgctxt "LNG_APP_LOGIN_CANCEL"
msgid "Cancel"
msgstr "Anuluj"

#. The message for the user. %1 is the server address, %2 is 'authentication realm' as sent in by server.
msgctxt "LNG_APP_LOGIN_MESSAGE"
msgid "Server %1 requests user name and password. The server says: %2."
msgstr "Serwer %1 żąda nazwy użytkownika i hasła. Komunikat serwera: %2."

#. Positive answer from the dialog
msgctxt "LNG_APP_LOGIN_OK"
msgid "Log in"
msgstr "Zaloguj się"

#. Label for password edit box
msgctxt "LNG_APP_LOGIN_PASSPHRASE"
msgid "&Pass phrase"
msgstr "&Hasło"

#. Title for Login dialog
msgctxt "LNG_APP_LOGIN_TITLE"
msgid "Password needed"
msgstr "Potrzebne hasło"

#. Label for user name edit box
msgctxt "LNG_APP_LOGIN_USERNAME"
msgid "&Login"
msgstr "&Login"

#. Help command. Shows dialog with name, version and copyright info.
msgctxt "LNG_APP_MENUITEM_ABOUTTASKS"
msgid "&About Tasks..."
msgstr "Zadania - i&nformacje"

#. Tooltip for LNG_APP_MENUITEM_ABOUTTASKS
msgctxt "LNG_APP_MENUITEM_ABOUTTASKS_TIP"
msgid "About Tasks"
msgstr "Zadania - informacje"

#. Connection command. Edits details of an exisiting connection
msgctxt "LNG_APP_MENUITEM_EDIT"
msgid "&Edit..."
msgstr "&Edytuj..."

#. Tasks command. Closes the application
msgctxt "LNG_APP_MENUITEM_EXIT"
msgid "E&xit"
msgstr "&Wyjdź"

#. Tooltip for LNG_APP_MENUITEM_EXIT
msgctxt "LNG_APP_MENUITEM_EXIT_TIP"
msgid "Exit"
msgstr "Wyjdź"

#. Connection command. Opens current issue in web browser
msgctxt "LNG_APP_MENUITEM_GOTO"
msgid "&Go To Issue"
msgstr "&Przejdź do Problemu"

#. Tasks submenu. Shows a list of available translations
msgctxt "LNG_APP_MENUITEM_LANGUAGES"
msgid "&Languages"
msgstr "&Języki"

#. Help command. Presents licences of all third party components
msgctxt "LNG_APP_MENUITEM_LICENSES"
msgid "Show &Licences"
msgstr "Pokaż &Licencje"

#. Tooltip for LNG_APP_MENUITEM_LICENSES
msgctxt "LNG_APP_MENUITEM_LICENSES_TIP"
msgid "Licences"
msgstr "Licencje"

#. Connection command. Logs time spent with current issue
msgctxt "LNG_APP_MENUITEM_LOGWORK"
msgid "Log &Work..."
msgstr "&Wykonana Praca..."

#. Languages submenu. Used for disabled action representing no languages at all
msgctxt "LNG_APP_MENUITEM_MISSING"
msgid "(Empty)"
msgstr "(puste)"

#. Tasks command. Starts adding a new connection to the task server.
msgctxt "LNG_APP_MENUITEM_NEWCONN"
msgid "New &Connection..."
msgstr "&Nowe połączenie..."

#. Tooltip for LNG_APP_MENUITEM_NEWCONN
msgctxt "LNG_APP_MENUITEM_NEWCONN_TIP"
msgid "New Connection"
msgstr "Nowe połączenie"

#. Connection command. Begins process of refreshing current table
msgctxt "LNG_APP_MENUITEM_REFRESH"
msgid "&Refresh"
msgstr "&Odśwież"

#. Tasks command. Begins process of refreshing all tables
msgctxt "LNG_APP_MENUITEM_REFRESHALL"
msgid "&Refresh All"
msgstr "&Odśwież wszystkie"

#. Tooltip for LNG_APP_MENUITEM_REFRESHALL
msgctxt "LNG_APP_MENUITEM_REFRESHALL_TIP"
msgid "Refresh All"
msgstr "Odśwież wszystkie"

#. Connection command. Deletes the connection and all its tables
msgctxt "LNG_APP_MENUITEM_REMOVE"
msgid "Re&move"
msgstr "Usuń"

#. Tasks command. Shows settings dialog
msgctxt "LNG_APP_MENUITEM_SETTINGS"
msgid "&Settings..."
msgstr "&Ustawienia..."

#. Tooltip for LNG_APP_MENUITEM_SETTINGS
msgctxt "LNG_APP_MENUITEM_SETTINGS_TIP"
msgid "Settings"
msgstr "Ustawienia"

#. Tray command. Allows to switch the application between visible/invisible states
msgctxt "LNG_APP_MENUITEM_SHOWHIDE"
msgid "Show/hide Tasks"
msgstr "Pokaż/usuń Zadania"

#. Languages submenu. Used for action representing the default user language
msgctxt "LNG_APP_MENUITEM_SYSLANG"
msgid "&System language (%1)"
msgstr "&Język systemu (%1)"

#. Top level menu name for connection commands
msgctxt "LNG_APP_MENU_CONNNECTIONS"
msgid "&Connections"
msgstr "&Połączenia"

#. Top level menu name for help commands
msgctxt "LNG_APP_MENU_HELP"
msgid "&Help"
msgstr "Pomo&c"

#. Top level menu name for application commands
msgctxt "LNG_APP_MENU_TASKS"
msgid "&Tasks"
msgstr "&Zadania"

#. Application name for window title
msgctxt "LNG_APP_NAME"
msgid "Tasks"
msgstr "Zadania"

#. Full window title. %1 is LNG_APP_NAME, %2 is version and %3 is the build number
msgctxt "LNG_APP_TITLE"
msgid "%1 (%2, build %3)"
msgstr "%1 (%2, kompilacja %3)"

#. Window title for elevated runs. %1 is LNG_APP_NAME or LNG_APP_TITLE
msgctxt "LNG_APP_TITLE_ELEVATED"
msgid "Administrator: %1"
msgstr "Administrator: %1"

#. Tooltip title for attention icon
msgctxt "LNG_ATTENTION_TOOLTIP"
msgid "Report(s) changed"
msgstr "Raport(y) zmienione"

#. Tooltip baloon notification name for added issues
msgctxt "LNG_JIRA_ADDED"
msgid "Added"
msgstr "Dodano"

#. Tooltip baloon notification name for changed issues
msgctxt "LNG_JIRA_CHANGED"
msgid "Changed"
msgstr "Zmieniono"

#. Table footer presenting the data slice; as if '(Issues 1-25 of 30)'
msgctxt "LNG_JIRA_ISSUES"
msgid "(Issues %1-%2 of %3)"
msgstr "(Zadania %1-%2 z %3)"

#. Column title for issue key - in case of field definition error; most of the time this string will come from JIRA server
msgctxt "LNG_JIRA_KEY"
msgid "Key"
msgstr "Klucz"

#. Tooltip baloon title for attention icon notification
msgctxt "LNG_JIRA_NOTIFY"
msgid "Changes in %1@%2"
msgstr "Zmiany w %1@%2"

#. Notification text for issue group. As if 'Added 4 issues'
msgctxt "LNG_JIRA_NOTIFY_GROUP"
msgid "%1 %2 issue"
msgid_plural "%1 %2 issues"
msgstr[0] "%1 jedno zadanie"
msgstr[1] "%1 %2 zadania"
msgstr[2] "%1 %2 zadań"

#. Notification text for issue group contents, used when group consists of more than two issues. As if 'KEY-1, KEY-2 and 5 others'
msgctxt "LNG_JIRA_NOTIFY_MORE"
msgid "%1, %2 and %3 other"
msgid_plural "%1, %2 and %3 others"
msgstr[0] "%1, %2 oraz jedno inne"
msgstr[1] "%1, %2 oraz %3 inne"
msgstr[2] "%1, %2 oraz %3 innych"

#. Notification text for issue group contents, used when group consists of two issues only. As if 'KEY-1 and KEY-2'
msgctxt "LNG_JIRA_NOTIFY_TWO"
msgid "%1 and %2"
msgstr "%1 i %2"

#. Tooltip baloon notification name for removed issues
msgctxt "LNG_JIRA_REMOVED"
msgid "Removed"
msgstr "Usunięto"

#. Replacement text for empty report
msgctxt "LNG_JIRA_REPORT_EMPTY"
msgid "Empty"
msgstr "Pusty"

#~ msgctxt "ATTR_LANGUAGE"
#~ msgid "Name of the language"
#~ msgstr "Polski"
